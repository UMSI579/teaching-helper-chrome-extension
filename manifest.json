{
  "name": "SI 579",
  "version": "1.0.0",
  "description": "Things to make running the class easier",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "579logo.png"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [{
    "matches": ["*://classroom.github.com/classrooms/*"],
    "js": ["content.js"]
  }],
  "externally_connectable": {
    "matches": ["*://classroom.github.com/classrooms/*"]
  }
}